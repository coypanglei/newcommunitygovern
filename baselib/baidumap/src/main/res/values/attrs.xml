<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="app_SuperRecyclerSingleAdapterView">
        <attr name="app_show_Swipe" format="boolean" />
        <attr name="app_show_add" format="boolean" />
        <attr name="app_add_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="app_map_show_points">
        <!--显示放大缩小按钮-->
        <attr name="app_show_zoom" format="boolean" />
        <!--手势控制-->
        <attr name="app_gesture_control" format="boolean" />
        <!--缩放比-->
        <attr name="app_map_zoom" format="float" />
        <!--显示定位点 默认定位一次 并显示再定位点-->
        <attr name="app_show_localing" format="boolean" />
        <!--地图跟随定位显示-->
        <attr name="app_follow_positioning_display" format="boolean" />
        <!--定位扫描间隔-->
        <attr name="app_localing_scan_span" format="integer" />
        <!--可点击的-->
        <attr name="app_clickable" format="boolean" />
        <!--点击再网格内-->
        <attr name="app_clickable_in_griding" format="boolean" />
    </declare-styleable>
</resources>