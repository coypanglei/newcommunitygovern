<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical">
    <include layout="@layout/public_include_title"/>
    <include layout="@layout/public_view_line_full" />

    <com.wq.newcommunitygovern.app.custom.SuperRecyclerSingleAdapterView
        android:id="@+id/super_recycler_adapter_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:app_show_Swipe="true" />
</LinearLayout>
